spawn ssh -o HostKeyAlgorithms=+ssh-rsa -o PubkeyAcceptedAlgorithms=+ssh-rsa -o StrictHostKeyChecking=no root@66.116.224.31 "chmod +x /root/disable_conflict.sh && /root/disable_conflict.sh"
expect {
    "password:" {
        send "$password\r"
        exp_continue
